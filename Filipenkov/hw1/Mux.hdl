/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    And(a=a, b=nsel, out=a_out);
    And(a=b, b=sel, out=b_out);
    Or(a=a_out, b=b_out, out=out);
}